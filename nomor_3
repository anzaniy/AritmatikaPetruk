#include <bits/stdc++.h>

using namespace std;

long unsigned int i;

bool Symbol(char OPERASI){
    if(OPERASI=='*' || OPERASI=='/' || OPERASI=='+' || OPERASI=='-' || OPERASI=='%'){
        return true; 
    } else {    
        return false; 
    }
}

int parse(char OPERASI){ 
    if(OPERASI=='+' || OPERASI=='-'){ 
        return 1; 
    }
    if(OPERASI=='*' || OPERASI=='/' ||OPERASI=='%'){ 
        return 2; 
    }return 0;
}

double applyOpr(double satu, double dua, char opr){
    switch(opr){
        case '*': return satu * dua;    
		case '/': return satu / dua; 
		case '+': return satu + dua; 
		case '-': return satu - dua;
        }
    return 0;
}
